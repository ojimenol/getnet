            <dependency>
                <groupId>es.santander.darwin</groupId>
                <artifactId>darwin-batch-integration-sftp-starter</artifactId>
                <version>${batch-integration.version}</version>
            </dependency>

            <!-- Santander Code Analysis Dependencies -->
            <dependency>
                <groupId>com.santander.framework.springboot</groupId>
                <artifactId>santander-code-analysis-library-rules</artifactId>
