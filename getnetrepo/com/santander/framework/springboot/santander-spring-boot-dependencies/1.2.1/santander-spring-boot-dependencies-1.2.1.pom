    <profiles>
        <profile>
            <id>spain-nexus</id>
            <activation>
                <property>
                    <name>!env.CODESPACES</name>
                </property>
            </activation>

            <distributionManagement>
                <snapshotRepository>
                    <id>nexus-snapshots</id>
                    <url>https://nexusmaster.alm.europe.cloudcenter.corp/repository/maven-snapshots/</url>
                </snapshotRepository>
                <repository>
                    <id>nexus-releases</id>
                    <url>https://nexusmaster.alm.europe.cloudcenter.corp/repository/maven-releases/</url>
                </repository>
            </distributionManagement>
        </profile>

        <profile>
            <id>integration-test</id>
            <build>
                <plugins>
                    <!-- disable native compilation in santander-modules when running integration tests -->
                    <plugin>
                        <groupId>org.graalvm.buildtools</groupId>
                        <artifactId>native-maven-plugin</artifactId>
                        <configuration>
                            <skipNativeTests>true</skipNativeTests>
                            <skip>true</skip>
                        </configuration>
                    </plugin>
                </plugins>
            </build>
        </profile>
    </profiles>

